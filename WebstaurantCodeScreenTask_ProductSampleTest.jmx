          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor" enabled="true">
            <stringProp name="TestPlan.comments">Extract unique product paths from list of product paths extracted from outlet page</stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">List&lt;String&gt; paths = new ArrayList();
int count = vars.get(&quot;productPagePaths_matchNr&quot;) as Integer;

for (def i = 0; i &lt; count; i++)
{
	paths.add(vars.get(&quot;productPagePaths_&quot; + (i+1)))
}

//remove duplicates
paths.unique();

//add paths to properties to make them available to test
count = 0;
for (path in paths)
{
	count++;
	props.put(&quot;newProductPagePaths_&quot; + count, path);
}

props.put(&quot;newProductPagePaths_matchNr&quot;, count as String);
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PostProcessor>
          <hashTree/>
        </hashTree>
