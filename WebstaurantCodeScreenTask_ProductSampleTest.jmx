            <stringProp name="RegexExtractor.refname">productPagePaths</stringProp>
            <stringProp name="RegexExtractor.regex">outlet/[a-zA-Z0-9]+/[0-9]+.html</stringProp>
            <stringProp name="RegexExtractor.template">$0$</stringProp>
            <stringProp name="RegexExtractor.default">productPagePaths_NotFound</stringProp>
            <stringProp name="RegexExtractor.match_number">-1</stringProp>
            <stringProp name="TestPlan.comments">regex product page paths from outlet page</stringProp>
          </RegexExtractor>
