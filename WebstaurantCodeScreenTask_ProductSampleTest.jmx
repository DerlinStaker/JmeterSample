              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">def numPaths = props.get(&quot;newProductPagePaths_matchNr&quot;) as int;

Random rnd = new Random();

def randIndex = rnd.nextInt(numPaths) + 1;

def randomProductPath = props.get(&quot;newProductPagePaths_&quot; + randIndex);

//log.info(&quot;randomProductPath &quot; + randomProductPath);

vars.put(&quot;randomProductPath&quot;, randomProductPath);</stringProp>
